<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="TOP_CENTER" stylesheets="@../styles/ServerDatabase.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.Server.GUI.Controllers.ServerDatabaseController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#refreshTables" text="Refresh">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></Button>
   </children>
</VBox>
